{
  "name": "project2-template",
  "version": "0.0.0",
  "private": true,
  "workspaces": [
    "client"
  ],
  "scripts": {
    "start": "node ./bin/www",
    "dev:all": "concurrently \"PORT=3000 nodemon ./bin/www\" \"PORT=5000 npm run start -w client\"",
    "dev:front": "PORT=5000 npm run start -w client",
    "dev:back": "PORT=3000 nodemon ./bin/www",
    "heroku-postbuild": "cd client && npm install && npm run build"
  },
  "dependencies": {
    "axios": "^0.21",
    "bcrypt": "^5",
    "bulma": "^0.9.3",
    "cloudinary": "^1",
    "connect-mongo": "^4",
    "cookie-parser": "^1",
    "cors": "^2.8.5",
    "dotenv": "^10",
    "express": "^4",
    "express-jwt": "^6.1.0",
    "express-session": "^1",
    "hbs": "^4",
    "http-errors": "^1",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5",
    "morgan": "^1",
    "multer": "^1",
    "multer-storage-cloudinary": "^4",
    "react-bulma-components": "^4.0.7",
    "react-icons": "^4.2.0",
    "react-responsive-carousel": "^3.2.21",
    "serve-favicon": "^2"
  },
  "devDependencies": {
    "concurrently": "^6",
    "debug": "^2",
    "nodemon": "^2"
  }
}
