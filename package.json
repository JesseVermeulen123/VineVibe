{
  "name": "project2-template",
  "version": "0.0.0",
  "private": true,
  "cacheDirectories": [
    "client/node_modules",
    "node_modules"
  ],
  "workspaces": [
    "client"
  ],
  "engines": {
    "node": "16.x",
    "npm": "7.x"
  },
  "scripts": {
    "start": "node ./bin/www",
    "dev:all": "concurrently \"PORT=3000 nodemon ./bin/www\" \"PORT=5000 yarn workspace client run start\"",
    "dev:front": "PORT=5000 yarn workspace client run start",
    "dev:back": " PORT=3000 nodemon ./bin/www",
    "build": "yarn workspace client run build"
  },
  "dependencies": {
    "axios": "^0.22",
    "bcrypt": "^5",
    "cloudinary": "^1",
    "connect-mongo": "^4",
    "cookie-parser": "^1",
    "cors": "^2.8.5",
    "dotenv": "^10",
    "express": "^4",
    "express-jwt": "^6.1.0",
    "express-session": "^1",
    "framer-motion": "^4.1.17",
    "hbs": "^4",
    "http-errors": "^1",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^6",
    "mongoose-paginate-v2": "^1.4.2",
    "morgan": "^1",
    "multer": "^1",
    "multer-storage-cloudinary": "^4",
    "serve-favicon": "^2",
    "stripe": "^8.178.0"
  },
  "devDependencies": {
    "concurrently": "^6",
    "debug": "^4",
    "nodemon": "^2"
  },
  "packageManager": "yarn@3.0.2"
}
